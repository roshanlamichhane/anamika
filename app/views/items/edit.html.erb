<h1>Anamika</h1>
<%= form_for @item, url: item_path(@item) do |f| %>
   <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>
<p>
    <%= f.label :quantity %><br>
    <%= f.text_area :quantity %>
  </p>
 
  <p>
    <%= f.label :quality %><br>
    <%= f.text_area :quality %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>
 <%= link_to 'Back', items_path %>